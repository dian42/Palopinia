<!DOCTYPE html>
<html lang="es">
<head>

    <meta charset="utf-8">
    <title> Importadora Palo Piña</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <!-- The styles -->
    {% stylesheets filter='cssrewrite' '@PpaPaloBundle/Resources/public/css/bootstrap-spacelab.css' %}
        <link  rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}

    <style type="text/css">
      body {
        padding-bottom: 40px;
      }
      .sidebar-nav {
        padding: 9px 0;
      }
    </style>
    {% stylesheets 
        '@PpaPaloBundle/Resources/public/css/bootstrap-responsive.css'
        '@PpaPaloBundle/Resources/public/css/charisma-app.css'
        '@PpaPaloBundle/Resources/public/css/jquery-ui-1.8.21.custom.css'
        '@PpaPaloBundle/Resources/public/css/fullcalendar.css'
     filter='cssrewrite' %}
        <link rel="stylesheet" href="{{ asset_url }}" >
    {% endstylesheets %}
    {% stylesheets 
        '@PpaPaloBundle/Resources/public/css/fullcalendar.print.css'
     filter='cssrewrite' %}
        <link media="print" rel="stylesheet" href="{{ asset_url }}" >
    {% endstylesheets %}
    {% stylesheets 
        '@PpaPaloBundle/Resources/public/css/chosen.css'
        '@PpaPaloBundle/Resources/public/css/uniform.default.css'
        '@PpaPaloBundle/Resources/public/css/colorbox.css'
        '@PpaPaloBundle/Resources/public/css/jquery.cleditor.css'
        '@PpaPaloBundle/Resources/public/css/jquery.noty.css'
        '@PpaPaloBundle/Resources/public/css/noty_theme_default.css'
        '@PpaPaloBundle/Resources/public/css/elfinder.min.css'
        '@PpaPaloBundle/Resources/public/css/elfinder.theme.css'
        '@PpaPaloBundle/Resources/public/css/jquery.iphone.toggle.css'
        '@PpaPaloBundle/Resources/public/css/opa-icons.css'
        '@PpaPaloBundle/Resources/public/css/uploadify.css'
        '@PpaPaloBundle/Resources/public/css/general.css'
     filter='cssrewrite' %}
        <link rel="stylesheet" href="{{ asset_url }}" >
    {% endstylesheets %}


    <!-- The HTML5 shim, for IE6-8 support of HTML5 elements -->
    <!--[if lt IE 9]>
      <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->

    <!-- The fav icon -->
    <link style="float:left" rel="shortcut icon" href="img/favicon.ico" width="200" heigh="200">
</head>

<body>
    <div class="navbar">
        <div class="navbar-inner">
            <div class="container-fluid">
                <a class="btn btn-navbar" data-toggle="collapse" data-target=".top-nav.nav-collapse,.sidebar-nav.nav-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </a>
                <a class="brand" href="{{path('home')}}"> 
                <img id="logo" style="float left;" src=" {{asset('bundles/ppapalo/img/logooficial.jpg')}}" hspace=3 align="middle"> <span style="clear:both">Sin ustedes quebramos</span>

                <!--<span id="textoLogo">Sin Usted Quebramos</span>--></a>             
              {% if is_granted('ROLE_ADMIN') %}
                 <a href="botoncrate"></a>
                 <div align="right" >
                 <a class="btn btn-success" href="{{path('user_logout')}}">
                    <i class="icon-off icon-white"></i>
                        Logout
                    </a>
                 <a class="btn btn-success" href="{{path('producto_new')}}">
                    <i class="icon-user icon-plus icon-white"></i>
                        Crear Producto
                    </a>
                 <a class="btn btn-success" href="{{path('tipoproducto_new')}}">
                    <i class="icon-tags icon-white"></i>
                        Crear Tipo Producto
                    </a>
                </div>
                {% endif %}               

     



                <!-- theme selector starts -->

                <!-- theme selector ends -->
                
                <!-- user dropdown starts -->

                <!-- user dropdown ends -->
                <!--
                <div class="top-nav nav-collapse">
                    <ul class="nav">
                        <li><a href="#">Visit Site</a></li>
                        <li>
                            <form class="navbar-search pull-left">
                                <input placeholder="Search" class="search-query span2" name="query" type="text">
                            </form>
                        </li>
                    </ul>
                </div>-->
                <!--/.nav-collapse -->
            </div>
        </div>
    </div>
    <!-- topbar ends -->
    <div class="container-fluid">
        <div class="row-fluid">
                
            <!-- left menu starts -->
            <div class="span2 main-menu-span">
                <div class="well nav-collapse sidebar-nav">
                    <ul class="nav nav-tabs nav-stacked main-menu">
                        <li class="nav-header hidden-tablet">Categoria</li>
                            {{ render(controller('PpaPaloBundle:Default:lista' )) }}
                            <li><a class="ajax-link" href="{{ path('contacto') }}"><i class="icon-eye-open"></i><span class="hidden-tablet"> Contacto</span></a></li>
                    </ul>
                </div><!--/.well -->
            </div><!--/span-->
            <!-- left menu ends -->
            
            


            
            <div id="content" class="span10">
            <!-- content starts -->
            

                {% block body%}

                {% endblock %}

          
       
                    <!-- content ends -->
            </div><!--/#content.span10-->
        </div><!--/fluid-row-->
                
        <hr>

        <div class="modal hide fade" id="myModal">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">×</button>
                <h3>Settings</h3>
            </div>
            <div class="modal-body">
                <p>Here settings can be configured...</p>
            </div>
            <div class="modal-footer">
                <a href="#" class="btn" data-dismiss="modal">Close</a>
                <a href="#" class="btn btn-primary">Save changes</a>
            </div>
        </div>

        <footer>
        </footer>
        
    </div><!--/.fluid-container-->

