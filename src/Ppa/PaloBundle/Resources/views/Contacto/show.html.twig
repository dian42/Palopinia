{% extends '::baseTablas.html.twig' %}

{% block body -%}
    <h1>Contacto</h1>

    <table id="DataTables_Table_0" class="table table-striped table-bordered">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ entity.nombre }}</td>
            </tr>
            <tr>
                <th>Apellido</th>
                <td>{{ entity.apellido }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ entity.email }}</td>
            </tr>
            <tr>
                <th>Telefono</th>
                <td>{{ entity.telefono }}</td>
            </tr>
            <tr>
                <th>Cargo</th>
                <td>{{ entity.cargo }}</td>
            </tr>
            <tr>
                <th>Telefono2</th>
                <td>{{ entity.telefono2 }}</td>
            </tr>
        </tbody>
    </table><br>
    <a class="btn" href="{{ path('contacto') }}">
        Ver la lista
    </a><br>
    <a class="btn btn-info" href="{{ path('contacto_edit', { 'id': entity.id }) }}">
        Editar
    </a>
    {{ form(delete_form) }}
{% endblock %}
